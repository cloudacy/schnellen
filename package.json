{
  "name": "schnellen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build --modern",
    "pretty": "prettier --write '*.{js,ts,vue,html,md,json,yml}' '{public,src,tests}/**/*.{js,ts,vue,html,md,json,yml}'",
    "release": "./release.sh",
    "start": "vue-cli-service serve --open"
  },
  "dependencies": {
    "averi-style": "^1.5.4",
    "v-icon": "^1.0.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-typescript": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "prettier": "^1.17.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "typescript": "^3.4.4",
    "vue-template-compiler": "^2.6.10"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "prettier": {
    "printWidth": 999,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "proseWrap": "never"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 99"
  ]
}
